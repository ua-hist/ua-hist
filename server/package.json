{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts | pino-pretty",
    "debug": "tsx --inspect src/index.ts",
    "brk": "tsx --inspect-brk src/index.ts",
    "exec": "node --experimental-specifier-resolution=node --loader ts-node/esm",
    "test": "vitest",
    "test:once": "node --import tsx --test test/**/*.spec.ts",
    "test:inspect": "vitest --inspect-brk --no-file-parallelism",
    "build": "tsc",
    "prod": "NODE_ENV=production node dist/index.js",
    "postinstall": "prisma generate",
    "db:drop": "prisma migrate reset",
    "db:migrate": "prisma migrate deploy",
    "db:seed": "yarn run exec prisma/seed.ts",
    "db:update": "prisma migrate dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@prisma/client": "^5.9.1",
    "fastify": "^4.26.1",
    "fastify-plugin": "^4.5.1",
    "vitest": "^1.2.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^1.5.4",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20.11.18",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "pino-pretty": "^10.3.1",
    "prisma": "^5.9.1",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  }
}
